{
  "rewrites": [
    {
      "source": "/src/login.html",
      "destination": "/src/login.html"
    },
    {
      "source": "/login.html",
      "destination": "/src/login.html"
    },
    {
      "source": "/",
      "destination": "/src/login.html"
    },
    {
      "source": "/index.html",
      "destination": "/index.html"
    },
    {
      "source": "/app",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ]
}